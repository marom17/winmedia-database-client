      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Winmedia_Database_Client"
      mc:Ignorable="d"
      Title="CatSearch"
      HorizontalAlignment="Stretch" VerticalAlignment="Stretch" d:DesignHeight="180" d:DesignWidth="716" Name="CatWin">

    <Grid>
        <TabControl VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Name="CatTab" MaxHeight="{Binding ElementName=CatWin, Path=ActualHeight}" MaxWidth="{Binding ElementName=CatWin, Path=ActualWidth}">
            
        </TabControl>

    </Grid>
</Page>
